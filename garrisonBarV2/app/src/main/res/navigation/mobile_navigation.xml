<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.garrisonbarv2.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.garrisonbarv2.ui.dashboard.DashboardFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.garrisonbarv2.ui.notifications.NotificationsFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_menu"
        android:name="com.example.garrisonbarv2.ui.menu.navigation_menu"
        android:label="navigation_menu"
        tools:layout="@layout/navigation_menu" />
    <fragment
        android:id="@+id/navigation_qrcode"
        android:name="com.example.garrisonbarv2.ui.qrcode.navigation_qrcode"
        android:label="navigation_qrcode"
        tools:layout="@layout/navigation_qrcode" />

</navigation>